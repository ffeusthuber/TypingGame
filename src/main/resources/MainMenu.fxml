<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="adapter.in.MainMenuController"
            prefHeight="640.0" prefWidth="480.0">
    <VBox alignment="center" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <Button text="Start Game" onAction="#handleStartButton"/>
        <Button text="How To Play" onAction="#handleHowToPlayButton"/>
        <Button text="Exit" onAction="#handleExitButton"/>
    </VBox>
</AnchorPane>